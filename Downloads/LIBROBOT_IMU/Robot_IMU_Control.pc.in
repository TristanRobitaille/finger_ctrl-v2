# Package Information for pkg-config

prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@/bin
libdir=@CMAKE_INSTALL_PREFIX@/@CMAKE_INSTALL_LIBDIR@
includedir=@CMAKE_INSTALL_PREFIX@/include

Name: libRobot_IMU_Control
Description: Open source library for controlling a Kinova Jaco robotic arm via USB
Version: @libRobot_IMU_Control_VERSION@
Libs: -L${libdir} -lRobot_IMU_Control -L@Boost_LIBRARY_DIRS@ -lboost_system
Cflags: -I${includedir}/libRobot_IMU_Control -I@Boost_INCLUDE_DIRS@
